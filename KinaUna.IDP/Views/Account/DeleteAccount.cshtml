@model KinaUna.IDP.Models.AccountViewModels.RegisterViewModel
@{
    ViewData["Title"] = "Confirm delete account";
}
<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-10">
        <div class="row">
            <div class="col-12 col-md-6 col-lg-5">
                A confirmation email has been sent to your email address: @Model.Email
            </div>
        </div>
        <div class="row">
            <div class="col-12 col-md-6 col-lg-5">
                Use the link in the email to confirm that you want to delete this account.
            </div>
        </div>
    </div>
</div>

