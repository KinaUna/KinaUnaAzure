@using KinaUna.Data
@model KinaUna.IDP.Models.AccountViewModels.LogoutViewModel
<h4>Logout</h4>
<div class="row">
    <div class="col-sm-6">
        <p>Would you like to logout of @Constants.AppName?</p>
        <form asp-action="Logout">
            <input type="hidden" name="logoutId" value="@Model.LogoutId"  />
            <fieldset>
                <div class="form-group">
                    <button class="btn btn-primary">Yes</button>
                </div>
            </fieldset>
        </form>
    </div>
</div>