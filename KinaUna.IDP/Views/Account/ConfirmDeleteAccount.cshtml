@model KinaUna.IDP.Models.AccountViewModels.RegisterViewModel
@{
    ViewData["Title"] = "Confirm Delete Account";
}
<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-10">
        <div class="row">
            <div class="col-12 col-md-6 col-lg-5">
                Thank you for your confirmation.
            </div>
        </div>
        <div class="row">
            <div class="col-12 col-md-6 col-lg-5">
                Your account has now been removed and all your data will be completely removed in 30 days.
            </div>
        </div>
    </div>
</div>

