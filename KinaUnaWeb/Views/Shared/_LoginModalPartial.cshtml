<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="loginModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-signup" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="loginModalClosed()">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <iframe name="loginFrame" id="loginFrame" src="about:blank" style="border: none; width: 100%; height: 550px;" onload="frameLoaded()"></iframe>
            </div>
        </div>
    </div>
</div>